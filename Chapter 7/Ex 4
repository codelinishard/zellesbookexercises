def main():
    print("This function checks the classification of a student by credit")
    credit = int(input("What is the student's credit?: "))
    if credit >=26:
        standing = "Senior"
    elif credit >= 16:
        standing = "Junior"
    elif credit >= 7:
        standing = "Sophomore"
    elif credit >= 0:
        standing = "Freshman"
    else: 
        print("Invalid credit")
        return
    print(f"the student's class standing is '{standing}'")
main()
